# Configuración para entorno de desarrollo
environment = "dev"

# Acceso público habilitado
enable_public_access = true
enable_port_forward  = true

# Zitadel siempre público
zitadel_always_public = true

# Grafana público en desarrollo
grafana_public_access = true

# Vault solo port-forward (no público)
vault_public_access = false 
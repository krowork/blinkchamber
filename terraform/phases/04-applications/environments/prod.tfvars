# Configuración para entorno de producción
environment = "prod"

# Acceso público deshabilitado
enable_public_access = false
enable_port_forward  = false

# Zitadel siempre público
zitadel_always_public = true

# Grafana no público en producción
grafana_public_access = false

# Vault no público en producción
vault_public_access = false 